{
  "expo": {
    "name": "LumaLife",
    "slug": "LumaLife",
    "version": "1.0.0",
    "backgroundColor": "#0f172a",
    "orientation": "portrait",
    "icon": "./assets/images/icons.png",
    "scheme": "lumalife",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "backgroundColor": "#0f172a"
      },
      "adaptiveIcon": {
        "backgroundColor": "#0f172a",
        "foregroundImage": "./assets/images/icons.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tanmayaggarwal.LumaLife",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.health.READ_HEALTH_DATA"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashs.png",
          "resizeMode": "contain",
          "backgroundColor": "#0f172a",
          "dark": {
            "backgroundColor": "#0f172a"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-health-connect",
        {
          "permissions": [
            "steps",
            "sleep",
            "distance",
            "exercise",
            "nutrition",
            "heart_rate"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55decf95-a8b4-4029-986b-fa40644f69a3"
      }
    },
    "owner": "tanmayaggarwal"
  }
}
